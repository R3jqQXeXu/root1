  <addon id="13927">
  <name>SEO Toolbar</name>
  <type id="1">Extension</type>
  <guid>{2e710e6b-5e9d-44ba-8f4e-09a040978b49}</guid>
  <slug>seo-toolbar-13927</slug>
  <version>1.3.5.1</version>
