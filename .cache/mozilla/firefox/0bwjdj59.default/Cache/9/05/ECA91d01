  <addon id="325597">
  <name>Silent Block</name>
  <type id="1">Extension</type>
  <guid>SilentBlock@schuzak.jp</guid>
  <slug>silentblock</slug>
  <version>1.2.2</version>
  <status id="4">Fully Reviewed</status>
  <authors>
        <author id="1390606">
      <name>Schuzak</name>
      <link>https://addons.mozilla.org/en-US/firefox/user/schuzak/?src=api</link>
